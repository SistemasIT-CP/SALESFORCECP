<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Kilometraje_numerico</fullName>
    <active>true</active>
    <description>El kilometraje debe ser numerico</description>
    <errorConditionFormula>AND(
  NOT(ISBLANK( Kilometraje__c )),
  NOT(ISNUMBER( Kilometraje__c ))
)</errorConditionFormula>
    <errorDisplayField>Kilometraje__c</errorDisplayField>
    <errorMessage>El kilometraje debe ser un numero</errorMessage>
</ValidationRule>
